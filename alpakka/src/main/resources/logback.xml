<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appender name="STDOUT" target="System.out" class="ch.qos.logback.core.ConsoleAppender"><encoder><pattern>[%date{ISO8601}] [%level] [%logger] - %msg%n</pattern></encoder></appender>
	<root level="INFO"><appender-ref ref="STDOUT"></appender-ref></root>	
</configuration>